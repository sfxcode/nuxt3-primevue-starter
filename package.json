{
  "name": "nuxt3-primevue-starter",
  "type": "module",
  "version": "2.3.0",
  "license": "MIT",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "info": "nuxi info",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "module": "nuxi init -t module my-module",
    "start": "node .output/server/index.mjs",
    "lint": "eslint ./app",
    "lint:fix": "eslint --fix ./app",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run --reporter verbose",
    "test:ci": "vitest --run --reporter verbose --exclude \"**/app.test.ts\"",
    "test:coverage": "vitest run --coverage",
    "postinstall": "nuxt prepare",
    "release": "npm run lint && npm run build && changelogen --minor --release && git push --follow-tags"
  },
  "devDependencies": {
