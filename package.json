    "@nuxt/eslint": "^1.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.11.0",
    "@nuxt/test-utils": "^3.19.2",
    "@pinia/colada": "^0.17.1",
